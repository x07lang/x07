{
  "suite_id": "abi-types-suite@0.1.0",
  "world": "solve-pure",
  "tasks": [
    {
      "task_id": "canary/echo",
      "description": "Return the input bytes unchanged.",
      "cases": [
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": ""
        },
        {
          "name": "misc",
          "input_b64": "aGkA/w==",
          "expected_b64": "aGkA/w=="
        }
      ]
    },
    {
      "task_id": "canary/reverse_bytes",
      "description": "Return the input bytes reversed (byte order reversed).",
      "cases": [
        {
          "name": "abcd",
          "input_b64": "YWJjZA==",
          "expected_b64": "ZGNiYQ=="
        },
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": ""
        }
      ]
    },
    {
      "task_id": "canary/sum_u8_u32le",
      "description": "Compute sum of all input bytes (u8) into a u32 (mod 2^32). Output is u32 little-endian (4 bytes).",
      "cases": [
        {
          "name": "123",
          "input_b64": "AQID",
          "expected_b64": "BgAAAA=="
        },
        {
          "name": "ff01",
          "input_b64": "/wE=",
          "expected_b64": "AAEAAA=="
        },
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": "AAAAAA=="
        }
      ]
    },
    {
      "task_id": "canary/max_u8_or_empty",
      "description": "If input is empty, output empty. Otherwise output a single byte equal to the maximum byte in the input.",
      "cases": [
        {
          "name": "123",
          "input_b64": "AQID",
          "expected_b64": "Aw=="
        },
        {
          "name": "0ff80",
          "input_b64": "AP+A",
          "expected_b64": "/w=="
        },
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": ""
        }
      ]
    },
    {
      "task_id": "canary/replace_byte_u8",
      "description": "Input format: [old:u8][new:u8][payload...]. Output = payload with every byte equal to old replaced by new.",
      "cases": [
        {
          "name": "1_to_9",
          "input_b64": "AQkBAgEB",
          "expected_b64": "CQIJCQ=="
        },
        {
          "name": "newline_to_cr",
          "input_b64": "Cg1hCmIK",
          "expected_b64": "YQ1iDQ=="
        }
      ]
    },
    {
      "task_id": "canary/filter_eq_u8",
      "description": "Input format: [target:u8][payload...]. Output all bytes from payload that equal target, preserving order.",
      "cases": [
        {
          "name": "zeros_small",
          "input_b64": "AAABAAIA",
          "expected_b64": "AAAA"
        },
        {
          "name": "a_in_banana",
          "input_b64": "YWJhbmFuYQ==",
          "expected_b64": "YWFh"
        },
        {
          "name": "alt4096",
          "input_b64": "",
          "expected_b64": "",
          "assertions": {
            "max_realloc_calls": 0,
            "max_memcpy_bytes": 4096,
            "max_peak_live_bytes": 65536
          }
        }
      ],
      "assertions": {
        "mem_stats_required": true,
        "leak_free_required": true
      }
    },
    {
      "task_id": "canary/stable_partition_lt_u8",
      "description": "Input format: [threshold:u8][payload...]. Output payload bytes stably partitioned: all bytes < threshold first, then all bytes >= threshold.",
      "cases": [
        {
          "name": "empty_payload",
          "input_b64": "AA==",
          "expected_b64": ""
        },
        {
          "name": "lt3_mixed",
          "input_b64": "AwEFAgQA",
          "expected_b64": "AQIABQQ="
        },
        {
          "name": "threshold0_no_lt",
          "input_b64": "AAECAwQ=",
          "expected_b64": "AQIDBA=="
        },
        {
          "name": "threshold255_only_255_at_end",
          "input_b64": "/wD/Af7/",
          "expected_b64": "AAH+//8="
        }
      ]
    },
    {
      "task_id": "canary/find_first_u8_u32le",
      "description": "Input format: [target:u8][payload...]. Output u32 little-endian index of first occurrence of target in payload, or 0xFFFFFFFF if not found.",
      "cases": [
        {
          "name": "a_in_banana",
          "input_b64": "YWJhbmFuYQ==",
          "expected_b64": "AQAAAA=="
        },
        {
          "name": "not_found",
          "input_b64": "AAECAw==",
          "expected_b64": "/////w=="
        },
        {
          "name": "at0",
          "input_b64": "AgICAg==",
          "expected_b64": "AAAAAA=="
        }
      ]
    },
    {
      "task_id": "canary/count_runs_u8_u32le",
      "description": "Count consecutive runs of equal bytes in the input. Example: [1,1,2,2,2,1] has runs [1],[2],[1] so result=3. Output u32 little-endian.",
      "cases": [
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": "AAAAAA=="
        },
        {
          "name": "112221",
          "input_b64": "AQECAgIB",
          "expected_b64": "AwAAAA=="
        },
        {
          "name": "0101",
          "input_b64": "AAEAAQ==",
          "expected_b64": "BAAAAA=="
        }
      ]
    },
    {
      "task_id": "h1/box_select_reverse",
      "description": "Input format: [mode:u8][payload...]. If mode==0, output payload unchanged. If mode!=0, output payload reversed. Preferred implementation: store payload in Box<bytes>, use borrowed views for reads, and respect move semantics (box.into consumes the box).",
      "cases": [
        {
          "name": "mode0_abc",
          "input_b64": "AGFiYw==",
          "expected_b64": "YWJj"
        },
        {
          "name": "mode1_abc",
          "input_b64": "AWFiYw==",
          "expected_b64": "Y2Jh"
        },
        {
          "name": "mode1_empty",
          "input_b64": "AQ==",
          "expected_b64": ""
        }
      ]
    },
    {
      "task_id": "h1/vec_dup4",
      "description": "Output the input repeated 4 times (concatenate input with itself 4\u00d7). Prefer Vec::with_capacity/reserve_exact (or equivalent) so realloc_calls stays 0.",
      "cases": [
        {
          "name": "ab",
          "input_b64": "YWI=",
          "expected_b64": "YWJhYmFiYWI=",
          "assertions": {
            "max_realloc_calls": 0,
            "max_memcpy_bytes": 256,
            "max_peak_live_bytes": 4096
          }
        },
        {
          "name": "0_255",
          "input_b64": "",
          "expected_b64": "",
          "assertions": {
            "max_realloc_calls": 0,
            "max_memcpy_bytes": 8192,
            "max_peak_live_bytes": 32768
          }
        }
      ],
      "assertions": {
        "mem_stats_required": true,
        "leak_free_required": true
      }
    },
    {
      "task_id": "h1/result_try_gate",
      "description": "Input format: [flag:u8][payload...]. If flag==0 return payload. Otherwise return empty. Preferred: represent this as Result<bytes,u32> and use (try ...) / early-return-on-Err expansion to exercise Result ABI and drop on early exits.",
      "cases": [
        {
          "name": "flag0_ok",
          "input_b64": "AG9r",
          "expected_b64": "b2s="
        },
        {
          "name": "flag1_err",
          "input_b64": "AW5v",
          "expected_b64": ""
        }
      ]
    },
    {
      "task_id": "h1/option_box_maybe_reverse",
      "description": "Input format: [flag:u8][payload...]. If flag==0 return empty. If flag!=0 return reverse(payload). Preferred: model this as Option<Box<bytes>> (None = no payload) and exercise the Option<Box<T>> niche optimization (null pointer = None).",
      "cases": [
        {
          "name": "flag0_abc",
          "input_b64": "AGFiYw==",
          "expected_b64": ""
        },
        {
          "name": "flag1_abc",
          "input_b64": "AWFiYw==",
          "expected_b64": "Y2Jh"
        },
        {
          "name": "flag1_empty",
          "input_b64": "AQ==",
          "expected_b64": ""
        }
      ]
    },
    {
      "task_id": "h1/view_first_line_len_u32le",
      "description": "Compute the length (in bytes) of the first line of input: bytes before the first '\\n' (0x0A). If there is no '\\n', the first line is the whole input. Output is u32 little-endian (4 bytes). Must avoid copying the input; a view/slice scan should be enough.",
      "cases": [
        {
          "name": "abc_nl_xyz",
          "input_b64": "YWJjCnh5eg==",
          "expected_b64": "AwAAAA==",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 8192
          }
        },
        {
          "name": "no_newline",
          "input_b64": "bm9fbmV3bGluZQ==",
          "expected_b64": "CgAAAA==",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 8192
          }
        },
        {
          "name": "just_newline",
          "input_b64": "Cg==",
          "expected_b64": "AAAAAA==",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 8192
          }
        },
        {
          "name": "empty",
          "input_b64": "",
          "expected_b64": "AAAAAA==",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 8192
          }
        }
      ],
      "assertions": {
        "mem_stats_required": true,
        "leak_free_required": true
      }
    },
    {
      "task_id": "h1/view_extract_window",
      "description": "Input format: [start:u32le][len:u32le][payload...]. Return payload[start : start+len] with clipping: if start>=payload_len => empty; otherwise copy min(len, payload_len-start) bytes. Preferred: make a view into payload and then copy exactly the output window (avoid copying the full payload).",
      "cases": [
        {
          "name": "2_3_abcdef",
          "input_b64": "AgAAAAMAAABhYmNkZWY=",
          "expected_b64": "Y2Rl",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 4096
          }
        },
        {
          "name": "4_10_abcdef",
          "input_b64": "BAAAAAoAAABhYmNkZWY=",
          "expected_b64": "ZWY=",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 4096
          }
        },
        {
          "name": "10_1_abcdef",
          "input_b64": "CgAAAAEAAABhYmNkZWY=",
          "expected_b64": "",
          "assertions": {
            "max_memcpy_bytes": 64,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 4096
          }
        },
        {
          "name": "4096_4096_payload16k",
          "input_b64": "",
          "expected_b64": "",
          "assertions": {
            "max_memcpy_bytes": 8192,
            "max_realloc_calls": 0,
            "max_peak_live_bytes": 32768
          }
        }
      ],
      "assertions": {
        "mem_stats_required": true,
        "leak_free_required": true
      }
    },
    {
      "task_id": "h1/interface_dispatch_checksum_u8",
      "description": "Input format: [mode:u8][payload...]. If mode==0 output SUM(payload) mod 256 as 1 byte. If mode!=0 output XOR(payload) as 1 byte. Preferred: implement SUM and XOR as two implementations of a small interface record (data_ptr + vtable_ptr) and call through it (dynamic dispatch).",
      "cases": [
        {
          "name": "sum_123",
          "input_b64": "AAECAw==",
          "expected_b64": "Bg=="
        },
        {
          "name": "xor_123",
          "input_b64": "AQECAw==",
          "expected_b64": "AA=="
        },
        {
          "name": "xor_empty",
          "input_b64": "AQ==",
          "expected_b64": "AA=="
        }
      ]
    }
  ]
}
